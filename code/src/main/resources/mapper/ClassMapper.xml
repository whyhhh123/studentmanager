<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.why.studentmanager.mapper.ClassMapper">
    <select id="findByClass" parameterType="int" resultType="Clazz">
        select * from s_class where id = #{id}
    </select>
    <select id="findAllClass" parameterType="int" resultType="Clazz">
        select * from s_class
    </select>

</mapper>